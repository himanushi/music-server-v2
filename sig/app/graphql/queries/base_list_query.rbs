module Queries
  class BaseListQuery < ::Queries::BaseQuery
    @direction: ::String
    @order: ::String
    @limit: ::Integer
    @offset: ::Integer
    def query: (
      conditions: ::Query::conditions,
      cursor: ::Query::cursor,
      sort: ::Query::sort
    ) -> untyped
    def list_query: (conditions: ::Query::conditions) -> ::Query::result
    def order: (
      untyped relation,
      cursor: ::Query::cursor,
      sort: ::Query::sort
    ) -> untyped
  end
end
